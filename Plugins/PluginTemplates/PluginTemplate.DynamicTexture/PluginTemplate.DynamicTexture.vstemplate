			<ProjectItem ReplaceParameters="true">AssemblyResolver.cs</ProjectItem>
			<ProjectItem ReplaceParameters="true" OpenInEditor="true">AtsMain.cs</ProjectItem>
			<ProjectItem ReplaceParameters="true">DynamicTexture.cs</ProjectItem>
		</Project>
	</TemplateContent>
	<WizardExtension>
		<Assembly>DetailManagerNET.PluginTemplatePack, Version=1.0.20220.1, Culture=Neutral, PublicKeyToken=aa349b8ff5518ce0</Assembly>
